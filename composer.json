{
    "name": "maaf/core",
    "description": "MAAF Core Framework - Dependency Injection, Module Loader, EventBus, Config Engine, HTTP Kernel, CLI",
    "type": "library",
    "keywords": ["maaf", "framework", "core", "di", "module-loader", "eventbus", "config", "http", "cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bloch TamÃ¡s",
            "email": "info@bloch.hu"
        }
    ],
    "require": {
        "php": ">=8.1",
        "php-di/php-di": "^6.1",
        "nikic/fast-route": "^1.3",
        "php-amqplib/php-amqplib": "^3.0",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1"
    },
    "suggest": {
        "phpunit/phpunit": "Required for testing"
    },
    "autoload": {
        "psr-4": {
            "MAAF\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MAAF\\Core\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
